{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[gemfile]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.wrappingIndent": "same"
  },
  "[postcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ruby]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "auto-rename-tag.activationOnLanguage": [
    "*"
  ],
  "cSpell.language": "en,en-GB,pt-BR",
  "cSpell.userWords": [
    "activerecord",
    "braintree",
    "caton",
    "cimg",
    "circleci",
    "codemods",
    "Concat",
    "csrf",
    "dokku",
    "Dokku",
    "envato",
    "flowtype",
    "gemfile",
    "github",
    "headlessui",
    "heroicons",
    "hljs",
    "lucascaton",
    "PGHOST",
    "PGPASSWORD",
    "PGPORT",
    "PGUSER",
    "postgres",
    "postgresql",
    "preconnect",
    "Pressable",
    "recaptcha",
    "recurly",
    "rollbar",
    "Rollbar",
    "rubocop",
    "semibold",
    "stylelint",
    "tailwindcss",
    "twilio"
  ],
  "diffEditor.renderSideBySide": false,
  "editor.cursorBlinking": "smooth",
  "editor.detectIndentation": false,
  "editor.fontFamily": "Hack Nerd Font, Menlo, Monaco, monospace",
  "editor.fontSize": 15,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.renderFinalNewline": "off",
  "editor.rulers": [
    80,
    100
  ],
  "editor.suggest.preview": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "erb": "html",
    "javascript": "javascriptreact",
    "postcss": "css"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx",
    "postcss": "css"
  },
  "eslint.alwaysShowStatus": true,
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.sortOrder": "fullPath",
  "explorer.openEditors.visible": 0,
  "files.associations": {
    "*.ejson": "json",
    "*.gemfile": "gemfile",
    "*.map": "json",
    "*.njk": "html",
    "*.tsbuildinfo": "json",
    ".env": "shellscript",
    ".env.production": "shellscript",
    ".prettierrc": "json",
    "/etc/hosts": "shellscript",
    "CODEOWNERS": "ignore",
    "Procfile": "yaml",
    "Procfile.dev": "yaml",
    "brakeman.ignore": "json",
    "config": "shellscript",
    "irbrc": "ruby",
    "pryrc": "ruby",
    "zshrc": "shellscript"
  },
  "files.exclude": {
    "**/.routes/routes.rb": true,
    "**/.routes/routes.txt": true
  },
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.openRepositoryInParentFolders": "always",
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.views.stashes.files.layout": "tree",
  "javascript.preferences.autoImportFileExcludePatterns": [
    "**/node_modules/@types/node/**"
  ],
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.autoRun": "off",
  "json.schemas": [
    {
      "fileMatch": [
        "cypress.json"
      ],
      "url": "https://on.cypress.io/cypress.schema.json"
    }
  ],
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "prettier.proseWrap": "always",
  "ruby.rubocop.useBundler": true,
  "rubyLsp.enabledFeatures": {
    "codeActions": true,
    "codeLens": true,
    "completion": true,
    "definition": true,
    "diagnostics": true,
    "documentHighlights": true,
    "documentLink": true,
    "documentSymbols": true,
    "foldingRanges": true,
    "formatting": true,
    "hover": true,
    "inlayHint": true,
    "onTypeFormatting": true,
    "references": true,
    "selectionRanges": true,
    "semanticHighlighting": true,
    "workspaceSymbol": true
  },
  "screencastMode.fontSize": 64,
  "search.collapseResults": "alwaysCollapse",
  "search.showLineNumbers": true,
  "security.workspace.trust.untrustedFiles": "open",
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.experimental.classRegex": [
    [
      [
        "clsx\\(([^]*)\\)",
        "(?:'|\"|`)([^\"'`]*)(?:'|\"|`)"
      ],
      "(?:'|\"|`)([^']*)(?:'|\"|`)"
    ]
  ],
  "terminal.external.osxExec": "iTerm 2.app",
  "terminal.integrated.fontSize": 15,
  "typescript.preferences.autoImportFileExcludePatterns": [
    "**/node_modules/@types/node/**"
  ],
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsicons.dontShowNewVersionMessage": true,
  "window.commandCenter": false,
  "window.newWindowDimensions": "maximized",
  "window.restoreWindows": "none",
  "workbench.colorTheme": "GitHub Dark",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "workbench.iconTheme": "vscode-icons",
  "workbench.layoutControl.enabled": false,
  "workbench.startupEditor": "newUntitledFile"
}