{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[gemfile]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120,
    "editor.wrappingIndent": "same"
  },
  "[postcss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[ruby]": {
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "cSpell.userWords": ["codemods", "flowtype"],
  "diffEditor.renderSideBySide": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "smooth",
  "editor.detectIndentation": false,
  "editor.fontFamily": "Hack Nerd Font, Menlo, Monaco, monospace",
  "editor.fontSize": 15,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": true,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.renderFinalNewline": false,
  "editor.rulers": [80, 100],
  "editor.suggest.preview": true,
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "erb": "html",
    "javascript": "javascriptreact",
    "postcss": "css"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx",
    "postcss": "css"
  },
  "eslint.alwaysShowStatus": true,
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "files.associations": {
    "*.ejson": "json",
    "*.gemfile": "gemfile",
    "*.map": "json",
    "*.njk": "html",
    "*.tsbuildinfo": "json",
    ".env": "shellscript",
    ".env.production": "shellscript",
    ".prettierrc": "json",
    "/etc/hosts": "shellscript",
    "CODEOWNERS": "ignore",
    "Procfile": "yaml",
    "Procfile.dev": "yaml",
    "brakeman.ignore": "json",
    "config": "shellscript",
    "irbrc": "ruby",
    "pryrc": "ruby",
    "zshrc": "shellscript"
  },
  "files.exclude": {
    "**/.routes/routes.rb": true,
    "**/.routes/routes.txt": true
  },
  "files.insertFinalNewline": true,
  "files.simpleDialog.enable": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.path": "/usr/local/bin/git",
  "gitlens.advanced.messages": {
    "suppressLineUncommittedWarning": true
  },
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.views.stashes.files.layout": "tree",
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.autoRun": "off",
  "json.schemas": [
    {
      "fileMatch": ["cypress.json"],
      "url": "https://on.cypress.io/cypress.schema.json"
    }
  ],
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "prettier.proseWrap": "always",
  "ruby.codeCompletion": "rcodetools",
  "ruby.format": "rubocop",
  "ruby.intellisense": "rubyLocate",
  "screencastMode.fontSize": 64,
  "search.collapseResults": "alwaysCollapse",
  "search.showLineNumbers": true,
  "security.workspace.trust.untrustedFiles": "open",
  "sync.autoDownload": true,
  "sync.autoUpload": true,
  "sync.forceUpload": false,
  "sync.gist": "a867a5f65c2f289539655a65eff24657",
  "sync.quietSync": true,
  "tailwindCSS.emmetCompletions": true,
  "terminal.external.osxExec": "iTerm 2.app",
  "terminal.integrated.fontSize": 15,
  "typescript.suggest.autoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsicons.dontShowNewVersionMessage": true,
  "window.newWindowDimensions": "maximized",
  "window.restoreWindows": "none",
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "GitHub Dark",
  "workbench.editor.enablePreview": false,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "newUntitledFile"
}
